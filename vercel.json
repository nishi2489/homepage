{
  "routes": [
    { "src": "^/images/(.*)", "dest": "/images/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build && npm run copy-images",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install"
} 